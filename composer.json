{
    "name": "weishaypt/laravel-lava-ru",
    "description": "lava.ru payments for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "weishaypt",
            "email": "ya@sanek.dev"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": "6.*",
        "guzzlehttp/guzzle": "~7.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Weishaypt\\LavaRu\\Facades\\": "src/Facades",
            "Weishaypt\\LavaRu\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
              "Weishaypt\\LavaRu\\LavaRuServiceProvider"
            ],
            "aliases": {
                "LavaRu": "Weishaypt\\LavaRu\\Facades\\LavaRu"
            }
        }
    }
}
